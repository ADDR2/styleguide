{
    "validateQuoteMarks": "'",
    "requireMultipleVarDecl": true,
    "maximumLineLength": {
      "value": 120,
      "allowComments": true,
      "allowRegex": true
    },
    "disallowMultipleLineStrings": true,
    "disallowFunctionDeclarations": true,
    "requireSpaceBeforeBinaryOperators": true,
    "requireSpaceAfterBinaryOperators": true,
    "validateIndentation": 4,
    "requireSpaceBeforeBlockStatements": true,
    "requireLineFeedAtFileEnd": true,
    "requireDotNotation": true,
    "requireCommaBeforeLineBreak": true,
    "disallowTrailingComma": true,
    "disallowAnonymousFunctions": true,
    "disallowMultipleLineComments": true,
    "requireNewLineBeforeLineComments": true,
    "disallowKeywordsOnParameters": ["arguments"],
    "disallowObjectCreation": true,
    "disallowArrayCreation": true,
    "disallowEqualAndNotEqualOperators": true,
    "requireDollarBeforejQueryAssignment": "ignoreProperties",
    "validateBlockStatements": true,
    "disallowImplicitTypeConversion": ["numeric", "binary", "string"],
    "requireMultipleLineInVarDeclaration": true,
    "disallowInitializationInVarDeclarations": true,
    "disallowBooleanCreation": true,
    "disallowNumberCreation": true
}